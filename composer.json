{
    "name":         "torii/torii2",
    "description":  "Simple portal framework",
    "minimum-stability": "dev",
    "repositories": [ {
        "type": "vcs",
        "url":  "https://github.com/Qafoo/REST-Micro-Framework"
    }, {
        "type": "composer",
        "url":  "http://packages.zendframework.com/"
    } ],
    "require":      {
        "arbit/periodic": "*",
        "kore/ctx-parser" : "*",
        "Qafoo/RMF": "dev-master",
        "twbs/bootstrap": "*",
        "twig/twig": "1.6.0",
        "kriswallsmith/buzz": "*",
        "zendframework/zendframework": "dev-master",
        "sabre/vobject" : "dev-master",
        "doctrine/common": "2.*",
        "doctrine/dbal": "2.*"
    },
    "config":       {
        "vendor-dir": "src/library"
    }
}
